        <ControlTemplate TargetType="{x:Type Separator}">
          <Border BorderBrush="White"
                  BorderThickness="1,1,1,1" Margin="5,0,5,0"/>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>  
  
