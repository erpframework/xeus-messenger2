				<Grid>
					<Rectangle Fill="{DynamicResource error2_design}" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Center"/>
					<TextBlock Text="{Binding Path=Message}" Foreground="{DynamicResource DefaultTextBrush2}" TextWrapping="Wrap" Margin="26,0,0,0" />
				</Grid>
			</Border>
    </DataTemplate>

    <DataTemplate DataType="{x:Type xeus2_xeus_Core:EventHeadlineMessage}">
        <Border Background="#FF1E1E1E" BorderBrush="{DynamicResource DefaultBorderBrush}" BorderThickness="1,1,1,1" CornerRadius="3,3,3,3" Padding="5,5,5,5" MaxWidth="300">
				<Grid>
					<Rectangle Fill="{DynamicResource news_design}" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Center"/>
					<StackPanel Margin="26,0,0,0" >
						<TextBlock Text="{Binding Path=Sender}" Foreground="{DynamicResource DefaultTextBrush2}" TextWrapping="Wrap" />
						<TextBlock Text="{Binding Path=Message}" Foreground="{DynamicResource DefaultTextBrush2}" TextWrapping="Wrap" TextTrimming="CharacterEllipsis" MaxWidth="100" MaxHeight="30" />
                    <Button Command="servicecmd:AccountCommands.DisplayHeadlines"
                        HorizontalAlignment="Right" VerticalAlignment="Bottom"
						Content="Display" Margin="0,5,0,0" />
                </StackPanel>
				</Grid>
			</Border>
    </DataTemplate>
	    
    <DataTemplate DataType="{x:Type xeus2_xeus_Core:EventErrorAuth}">
        <Border Background="#FF1E1E1E" BorderBrush="{DynamicResource DefaultBorderBrush}" BorderThickness="1,1,1,1" CornerRadius="3,3,3,3" Padding="5,5,5,5" MaxWidth="300">
        	<Grid>
        		<Rectangle Fill="{DynamicResource register_design}" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,4,0,0"/>
        		<Rectangle Fill="{DynamicResource error2_design}" Height="14" VerticalAlignment="Top" Margin="8,0,6,0" HorizontalAlignment="Left" Width="14"/>
        		<TextBlock Text="You were not authorized. Check your user name and password." Foreground="{DynamicResource DefaultTextBrush2}"
				 TextWrapping="Wrap" Margin="26,0,0,0" />
				 <Button Command="servicecmd:AccountCommands.DisplayLogin"
                        HorizontalAlignment="Right" VerticalAlignment="Bottom"
						Content="Retry" Margin="0,20,0,0" />
        	</Grid>
			</Border>
    </DataTemplate>

