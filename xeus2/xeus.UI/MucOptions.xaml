    <Grid>
      <TabControl Foreground="{DynamicResource DefaultTextBrush2}">
        <TabItem Foreground="{DynamicResource chat_back_design}">
          <TabItem.Header>
            <Border Padding="5,5,5,5">
              <Grid>
                <Rectangle Width="30" Height="20" Fill="{DynamicResource muc_affs_design}" HorizontalAlignment="Left"/>
                <TextBlock Text="User Affiliations" Margin="35,0,0,0" VerticalAlignment="Center"/>
              </Grid>
            </Border>
          </TabItem.Header>
          <WrapPanel>
            <xeus2_xeus_Controls:MucAffiliationList x:Name="_affOwner"/>
            <xeus2_xeus_Controls:MucAffiliationList x:Name="_affAdmin"/>
            <xeus2_xeus_Controls:MucAffiliationList x:Name="_affBanned"/>
            <xeus2_xeus_Controls:MucAffiliationList x:Name="_affMembers"/>
          </WrapPanel>
        </TabItem>
        <TabItem Foreground="{DynamicResource chat_back_design}">
          <TabItem.Header>
            <Border Padding="5,5,5,5">
              <Grid>
                <Rectangle Width="35" Fill="{DynamicResource muc_options_design}" HorizontalAlignment="Left" Height="25"/>
                <TextBlock Text="Room Options" Margin="38,0,0,0" VerticalAlignment="Center"/>
              </Grid>
            </Border>
          </TabItem.Header>
          <Grid Margin="0,5,0,0">
            <xeus2_xeus_XData:MucOptionsForm x:Name="_muc" Margin="0,0,0,40"/>
            <StackPanel Margin="0,0,0,5" VerticalAlignment="Bottom" Orientation="Horizontal" HorizontalAlignment="Center">
              <Button Content="Save Configuration"
                      Click="OnSaveConfig" Margin="0,0,5,0"/>
              <Button Content="Reset Configuration"
                      Click="OnResetConfig" Margin="5,0,0,0"/>
            </StackPanel>
          </Grid>
        </TabItem>
      </TabControl>   
    </Grid>
</Window>
