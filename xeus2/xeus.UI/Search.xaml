<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:xeus2_xeus_XData="clr-namespace:xeus2.xeus.XData"
    x:Class="xeus2.xeus.UI.Search"
    Title="xeus2.xeus.UI" Width="400"
    MinWidth="400" ResizeMode="CanResizeWithGrip" SizeToContent="Height" Background="{DynamicResource back_x_design}"
    >
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="..\xeus.XData\BasicStyles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Window.Resources>
  <StackPanel>
    <xeus2_xeus_XData:SearchService x:Name="_search"/>
    <Button Click="OnSearch">Search</Button>
  	<xeus2_xeus_XData:XDataResultView x:Name="_resultView" Height="200"/>
  </StackPanel>
</Window>
