      <DataTrigger Binding="{Binding Path=TypicalEventCode}">
        <DataTrigger.Value>
          <xeus2_xeus_Core:TypicalEvent>Left</xeus2_xeus_Core:TypicalEvent>
        </DataTrigger.Value>
        <Setter Property="Fill" TargetName="_code" Value="{StaticResource event_left_muc_design}"/>
      </DataTrigger>
      <DataTrigger Binding="{Binding Path=TypicalEventCode}">
        <DataTrigger.Value>
          <xeus2_xeus_Core:TypicalEvent>NickChange</xeus2_xeus_Core:TypicalEvent>
        </DataTrigger.Value>
        <Setter Property="Fill" TargetName="_code" Value="{StaticResource event_nickchange_muc_design}"/>
      </DataTrigger>      
      <DataTrigger Binding="{Binding Path=TypicalEventCode}">
        <DataTrigger.Value>
          <xeus2_xeus_Core:TypicalEvent>Banned</xeus2_xeus_Core:TypicalEvent>
        </DataTrigger.Value>
        <Setter Property="Fill" TargetName="_code" Value="{StaticResource aff_outcast_design}"/>
      </DataTrigger>
      <DataTrigger Binding="{Binding Path=TypicalEventCode}">
        <DataTrigger.Value>
          <xeus2_xeus_Core:TypicalEvent>Kicked</xeus2_xeus_Core:TypicalEvent>
