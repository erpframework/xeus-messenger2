  <DataTemplate x:Key="MucContact">
	  <Grid ToolTip="{Binding Mode=OneWay}">
      	<Rectangle x:Name="_affiliation" Width="24" Height="24" HorizontalAlignment="Left" VerticalAlignment="Center"
			Fill="{StaticResource aff_none_design}" Margin="0,0,0,5" />
      	<Rectangle x:Name="_show" Width="16" Height="16" HorizontalAlignment="Left" VerticalAlignment="Bottom"			/>
      	<TextBlock Text="{Binding Path=Nick}" Margin="24,0,0,0" Foreground="{DynamicResource DefaultTextBrush2}"/>
	  </Grid>
	
  	<DataTemplate.Triggers>
		<DataTrigger Binding="{Binding Path=Affiliation}" Value="admin">
  			<Setter Property="Fill" TargetName="_affiliation" Value="{StaticResource aff_admin_design}"/>
  		</DataTrigger>
		<DataTrigger Binding="{Binding Path=Affiliation}" Value="owner">
  			<Setter Property="Fill" TargetName="_affiliation" Value="{StaticResource aff_owner_design}"/>
  		</DataTrigger>
		<DataTrigger Binding="{Binding Path=Affiliation}" Value="outcast">
  			<Setter Property="Fill" TargetName="_affiliation" Value="{StaticResource aff_outcast_design}"/>
  		</DataTrigger>
		<DataTrigger Binding="{Binding Path=Affiliation}" Value="member">
  			<Setter Property="Fill" TargetName="_affiliation" Value="{StaticResource aff_member_design}"/>
  		</DataTrigger>
		<DataTrigger Binding="{Binding Path=Show}" Value="dnd">
  			<Setter Property="Fill" TargetName="_show" Value="{StaticResource dnd_design}"/>
  		</DataTrigger>
		<DataTrigger Binding="{Binding Path=Show}" Value="away">
  			<Setter Property="Fill" TargetName="_show" Value="{StaticResource away_design}"/>
  		</DataTrigger>
		<DataTrigger Binding="{Binding Path=Show}" Value="xa">
  			<Setter Property="Fill" TargetName="_show" Value="{StaticResource xaway_design}"/>
  		</DataTrigger>
    </DataTemplate.Triggers>	
  </DataTemplate>

	<DataTemplate  DataType="{x:Type xeus2_xeus_Core:MucContact}">
	    <StackPanel Margin="5,5,5,5">
			<Grid Margin="0,3,0,0">
				<TextBlock Text="MUC Status" Foreground="{DynamicResource DefaultTextBrush2}"/>
			   	<TextBlock Text="{Binding Path=MucStatusCodeText}" Foreground="{DynamicResource DefaultTextBrush2}" TextWrapping="Wrap" Margin="90,0,0,0"/>
			</Grid>
			<Grid Margin="0,3,0,0">
				<TextBlock Text="Status" Foreground="{DynamicResource DefaultTextBrush2}"/>
			   	<TextBlock Text="{Binding Path=StatusText}" Foreground="{DynamicResource DefaultTextBrush2}" TextWrapping="Wrap" Margin="90,0,0,0"/>
			</Grid>
			<Grid Margin="0,3,0,0">
				<TextBlock Text="Affiliation" Foreground="{DynamicResource DefaultTextBrush2}"/>
			   	<TextBlock Text="{Binding Path=Affiliation}" Foreground="{DynamicResource DefaultTextBrush2}" TextWrapping="Wrap" Margin="90,0,0,0"/>
			</Grid>
			<Grid Margin="0,3,0,0">
				<TextBlock Text="Group" Foreground="{DynamicResource DefaultTextBrush2}"/>
			   	<TextBlock Text="{Binding Path=Group}" Foreground="{DynamicResource DefaultTextBrush2}" TextWrapping="Wrap" Margin="90,0,0,0"/>
			</Grid>
		</StackPanel>
	</DataTemplate>
	<Style x:Key="BasicFlowDocumentScrollViewerStyle" TargetType="{x:Type FlowDocumentScrollViewer}">
		<Setter Property="Background" Value="#00FFFFFF"/>
		<Setter Property="ContextMenu" Value="{DynamicResource {ComponentResourceKey ResourceId=PUIFlowViewers_ContextMenu, TypeInTargetAssembly={x:Type System_Windows_Documents:PresentationUIStyleResources}}}"/>
		<Setter Property="HorizontalContentAlignment" Value="Stretch"/>
		<Setter Property="VerticalContentAlignment" Value="Stretch"/>
		<Setter Property="KeyboardNavigation.TabNavigation" Value="Local"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type FlowDocumentScrollViewer}">
					<Grid Language="en-us">
						<Grid.RowDefinitions>
							<RowDefinition Height="*"/>
							<RowDefinition Height="Auto"/>
						</Grid.RowDefinitions>
						<Border Margin="{TemplateBinding Padding}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
							<ScrollViewer HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" x:Name="PART_ContentHost" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" CanContentScroll="True"/>
						</Border>
						<Border x:Name="PART_ToolBarHost" Background="{TemplateBinding Background}" Grid.Row="1">
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="*"/>
									<ColumnDefinition Width="*"/>
								</Grid.ColumnDefinitions>
								<Border Visibility="Collapsed" HorizontalAlignment="Left" x:Name="PART_FindToolBarHost" VerticalAlignment="Center" Grid.Column="0"/>
								<Grid HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="1">
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="Auto"/>
										<ColumnDefinition/>
										<ColumnDefinition Width="Auto"/>
									</Grid.ColumnDefinitions>
									<RepeatButton IsEnabled="{TemplateBinding CanDecreaseZoom}" ToolTip="Decrease Zoom" Width="{TemplateBinding FontSize}" Height="{TemplateBinding FontSize}" BorderBrush="{TemplateBinding BorderBrush}" Foreground="{TemplateBinding Foreground}" Command="NavigationCommands.DecreaseZoom" Grid.Column="0">
										<RepeatButton.LayoutTransform>
											<ScaleTransform ScaleX="2.2" ScaleY="2.2"/>
										</RepeatButton.LayoutTransform>
										<RepeatButton.Style>
											<Style TargetType="{x:Type RepeatButton}">
												<Setter Property="Padding" Value="2,2,0,2"/>
												<Setter Property="Template">
													<Setter.Value>
														<ControlTemplate TargetType="{x:Type RepeatButton}">
															<Border Background="#00FFFFFF" Padding="{Binding Path=Padding, RelativeSource={RelativeSource TemplatedParent}}">
																<Viewbox RenderTransformOrigin="0.5,0.5" x:Name="Content">
																	<Canvas Width="32" Height="32">
																		<Path Fill="{DynamicResource back_alt}" x:Name="Sheen" Width="32" Height="32">
																			<Path.Data>
																				<GeometryGroup>
																					<EllipseGeometry Center="16,16" RadiusX="16" RadiusY="16"/>
																				</GeometryGroup>
																			</Path.Data>
																		</Path>
																		<Path Stroke="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" StrokeThickness="5" Canvas.Left="16" Canvas.Top="16">
																			<Path.Data>
																				<LineGeometry EndPoint="10,0" StartPoint="-10,0"/>
																			</Path.Data>
																		</Path>
																	</Canvas>
																</Viewbox>
															</Border>
														</ControlTemplate>
